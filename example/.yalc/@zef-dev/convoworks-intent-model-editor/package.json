{
  "name": "@zef-dev/convoworks-intent-model-editor",
  "version": "1.4.2+8c38633d",
  "description": "Intent and entity editors for the Convoworks framework",
  "author": "zef-dev",
  "license": "MIT",
  "repository": "zef-dev/convoworks-intent-model-editor",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --css-modules false",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@yaireo/tagify": "^3.21.5",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "rangy": "^1.3.0",
    "react-autocomplete-input": "^1.0.15",
    "react-contenteditable": "^3.3.5",
    "react-cool-onclickoutside": "^1.5.9",
    "react-html-parser": "^2.0.2",
    "react-is": "^17.0.1",
    "react-svg": "^12.1.0",
    "sanitize-html": "^2.3.0"
  },
  "yalcSig": "8c38633d8dc0f50bb4e4c45b6da34ec9"
